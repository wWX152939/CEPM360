<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:android1="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" android:background="@drawable/page_background"
    android1:orientation="vertical">

	<TextView
        android1:id="@+id/textView1"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_marginTop="@dimen/dp50_h"
        android1:layout_alignParentTop="true"
        android1:layout_centerHorizontal="true"
        android1:textSize="@dimen/sp50_s"
        android1:text="@string/welcome" />
        
    <CalendarView
        android1:id="@+id/calendarView1"
        android1:layout_width="match_parent"
        android1:layout_height="match_parent"
        android1:layout_marginTop="@dimen/dp200_h"
        android1:layout_marginBottom="@dimen/dp10_h"
        android1:layout_marginLeft="@dimen/dp400_w"
        android1:layout_marginRight="@dimen/dp180_w"
        android:background="@android:color/white"
        android1:layout_alignParentLeft="true" />

     		
</RelativeLayout>

